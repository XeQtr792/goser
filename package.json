{
  "name": "goser",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/XeQtr792/goser.git",
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "husky": "^6.0.0"
  },
  "scripts": {
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
